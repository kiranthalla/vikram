package mahesh1;

import java.util.Scanner;

public class kiran1
{
	public static void main(String[] args)
	{
		Scanner Scan =new Scanner(System.in);
		int n=Scan.nextInt();
		int m=Scan.nextInt();
		int l=Scan.nextInt();
		if(n>m && n>l)
		{
			System.out.println(n);
		}
		else if(m>l && m>n)
		{
			System.out.println(m);
		}
		else if(l>n  && l>m);
		{
			System.out.println(l);
		}
		}
}